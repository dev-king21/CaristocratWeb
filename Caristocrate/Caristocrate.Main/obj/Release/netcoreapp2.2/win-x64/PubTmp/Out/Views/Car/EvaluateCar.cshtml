        $(".uploadInnerdiv").find("a").each(function () { $(this).css("background-image", "none") });
    }

    function GetModels($this) {

        var makeid = $($this).find("option:selected").val();

        $("#ddlModelMC").find("option").each(function () {
            $(this).hide();
        })

        $("#ddlModelMC").val(0);

        $("#ddlModelMC").find("option").each(function () {
            if ($(this).attr("brandID") == makeid) {
                $(this).show();
            }
        })
    }

