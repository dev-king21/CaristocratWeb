<script type="text/javascript">
    $(document).ready(function () {

        $(".Newschildlisting").each(function () {
            $(this).hide();
        })

        for (var i = 1; i <= 10; i++) {
            $(".Newschildlisting").each(function () {
                if ($(this).attr("rowno") == i) {
                    $(this).show();
                }
            })
        }
    });

    function GotoNewsDetail($this) {
        var href = $($this).siblings().find("a").attr("href");
        window.location.href = href;
    }

function MarkFavourite(id, type, $this) {

        var text = $($this).text();

        var data = {};
        data.news_id = id;
        data.type = type;

