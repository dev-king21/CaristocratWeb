                                    <th class="fixed-side" scope="col" colspan="100">Dimensions & Weight</th>
                                </tr>

                                <tr>
                                    <td class="fixed-side" scope="col"><strong class="light">Length</strong></td>
                                    @foreach (var item in Model.data)
                                    {
                                        <td>
                                            @if (item.limited_edition_specs_array != null)
                                            {
                                                if (item.limited_edition_specs_array.Dimensions_Weight != null)
                                                {
                                                    if (item.limited_edition_specs_array.Dimensions_Weight.Count > 0)
                                                    {
                                                        foreach (var subitem in item.limited_edition_specs_array.Dimensions_Weight)
                                                        {
                                                            if (subitem.name == "LENGTH")
                                                            {
                                                                if (subitem.is_highlight == 1)
                                                                {
                                                                    <span class="hlighted">
                                                                        @(subitem.value + " MM")
                                                                    </span>
                                                                }
                                                                else
                                                                {
                                                                    @(subitem.value + " MM")
