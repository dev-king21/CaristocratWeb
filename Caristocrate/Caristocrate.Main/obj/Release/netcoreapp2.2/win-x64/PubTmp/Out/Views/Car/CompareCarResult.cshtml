                                foreach (var item in Model.data.SkipLast(1))
                                {
                                    <strong>@item.name</strong><span>&nbsp;VS&nbsp;</span>
                                }
                                foreach (var item in Model.data.TakeLast(1))
                                {
                                    <strong>@item.name</strong>
                                }
                            }
                        }
