                        <table class="table main-table">
                            <thead>
                                <tr>
                                    <th class="fixed-side" scope="col" rowspan="2">Car <br> Details</th>
                                    @foreach (var item in Model.data)
                                    {
                                        <th class="carimagetd" style="border-bottom:2px solid white;">
                                            <figure><img width="150" src='@item.media.FirstOrDefault().file_url' class="img-fluid" alt=""></figure>
                                        </th>
                                    }
                                </tr>
                                <tr>
                                    <th class="fixed-side" scope="col" style="display:none;"></th>
                                    @foreach (var item in Model.data)
